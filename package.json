{
  "name": "reporter",
  "version": "1.0.0",
  "description": "Transform mint transactions into summary csvs for use in a tracking sheet",
  "main": "reporter.js",
  "scripts": {
    "tsc": "tsc",
    "reporter": "npm run tsc && START_DATE=06/01/2024 END_DATE=06/30/2024 node dist/reporter.js",
    "dev": "FILE_EXTS=.CSV START_DATE=01/01/2024 END_DATE=01/31/2024 nodemon dist/reporter.js",
    "test": "FILE_EXTS=.test START_DATE=01/01/2000 END_DATE=01/01/3000 nodemon dist/reporter.js"
  },
  "author": "Jimmy Kuruvilla",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "nodemon": "^3.1.0"
  }
}